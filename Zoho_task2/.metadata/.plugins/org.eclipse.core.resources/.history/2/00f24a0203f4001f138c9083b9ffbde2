package zoho_task2_1;

public class Type_Conversions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		long long1 = 1234;
		int int1 = 8;
		float float1 = 7.4f;
		double double1 = 2.4;
		short short1 = 12;
		byte byte1 = 9;
		
		long i2l = (long)int1;
		long f2l = (long)float1;
		long d2l = (long)double1;
		long s2l = (long)short1;
		long b2l = (long)byte1;
		
		int l2i = (int)long1;
		int f2i = (int)float1;
		int d2i = (int)double1;
		int s2i = (int)short1;
		int b2i = (int)byte1;
		
		int l2i = (int)long1;
		int f2i = (int)float1;
		int d2i = (int)double1;
		int s2i = (int)short1;
		int b2i = (int)byte1;
		
		int l2i = (int)long1;
		int f2i = (int)float1;
		int d2i = (int)double1;
		int s2i = (int)short1;
		int b2i = (int)byte1;
		
		int l2i = (int)long1;
		int f2i = (int)float1;
		int d2i = (int)double1;
		int s2i = (int)short1;
		int b2i = (int)byte1;
		
		int l2i = (int)long1;
		int f2i = (int)float1;
		int d2i = (int)double1;
		int s2i = (int)short1;
		int b2i = (int)byte1;
		
	}

}
